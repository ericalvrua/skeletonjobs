
<div>
    <form action="{{ accion }}" method="post" class="formulario" onsubmit="manejador(event)">
        <label for="cif">CIF: </label>
        <input type="text" name="cif" id="cif" required><br><br>

        <label for="nombre">Nombre: </label>
        <input type="text" name="nombre" id="nombre" required><br><br>

        <label for="correo">Correo electronico: </label>
        <input type="email" name="correo" id="correo" required><br>

        <label for="correo2">Repita el correo: </label>
        <input type="email" name="correo2" id="correo2" required><br><br>

        <label for="pass">Contraseña: </label>
        <input type="password" name="pass" id="pass" required><br>
        
        <label for="pass2">Repita la contraseña: </label>
        <input type="password" name="pass2" id="pass2" required><br><br>
        

        <label for="telefono">Telefono: </label>
        <input type="tel" name="telefono" id="telefono" required><br><br>

        <input style="width:60%;" class="button primary" type="submit" value="Registrarse">
    </form>
</div>